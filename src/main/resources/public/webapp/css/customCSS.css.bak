} 

/* ---------------------------------------------------
    SIDEBAR STYLE
----------------------------------------------------- */

.wrapper {
    display: flex;
    width: 100%;
    align-items: stretch;
    height: 100vh;
}

#sidebar {
    min-width: 250px;
    max-width: 100%;
    
    background: rgb(255, 255, 255);
    color: #004D96;
    transition: all 0.3s;
    
}

#sidebar-right {
  min-width: 250px;
  max-width: 100%;
  height: 100vh; 
  background: rgb(255, 255, 255);
  color: #004D96;
  transition: all 0.3s;
}

#sidebar.active {
    margin-left: -250px;
}


#sidebar ul.components {
    padding: 20px 0;
    list-style: none;
    /*border-bottom: 1px solid #47748b;*/
}


