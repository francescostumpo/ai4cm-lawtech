  background-color: rgba(77, 199, 236, 0.475) !important;
}

.break { 
  background: #3E87BC;
  height: 1px;
  margin: 5px 0 5px 0;
  width: 100%;
}

.highlightDate{
  background-color: #d5f4e6;
} 

.highlightContact{
