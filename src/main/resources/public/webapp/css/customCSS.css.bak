}

.fixed-top-custom{
  position: fixed;
  right: 0;
  left: 0;
  margin-bottom: 80px;
  z-index: 1030;
}

.sticky-top-custom{
  position: -webkit-sticky;
  position: sticky;
  top: 80px;
  z-index: 1030; 
  background-color: white;
}

.sticky-top-custom-card{
  position: -webkit-sticky;
  position: relative;
  top: 250px;
  z-index: 1020;
}
#toTopButton {
  display: none;
  position: fixed;
  bottom: 20px;
  right: 30px;
  z-index: 99;
  cursor: pointer;
}

.highlightTextTemp{
  /*background-color: #a9d18e;*/
