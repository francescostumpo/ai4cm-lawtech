}

.uploaded {
  border-color: #00B042;
  color: #00B042;
}

.not-uploaded {
  border-color: #FD0000;
  color: #FD0000;
}

.file-upload > input{
  display: none;
}

#loading {
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  position: fixed;
  display: block;
  opacity: 0.95;
  background-color:#F0F0FC;
  z-index: 99;
  text-align: center;
}

.message {
  position: fixed;
  z-index: 999;
  height: 2em;
  width: 100%;
  overflow: show;
  margin: auto;
  padding-top: 4em;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  text-align: center;
}

#loading_1 {
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  position: fixed;
  display: block;
  opacity: 0.95;
  background-color:#F0F0FC;
  z-index: 99;
  text-align: center;
}



#loading-image {
  position: absolute;
  top: 50%;
  left: 50%;
  -moz-transform: translateX(-50%) translateY(-50%);
  -webkit-transform: translateX(-50%) translateY(-50%);
  transform: translateX(-50%) translateY(-50%);
}

.badge-grouped {
  display: flex;
  justify-content: space-between;
  width: 100%;
}

.my-custom-scrollbar {
  position: relative;
  overflow: auto;
}

.height {
  height: 230px;
}

.max-height {
